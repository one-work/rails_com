<%= render 'show_table_form' %>
<div class="level">
  <div class="level-left"></div>
  <div class="level-right">
    <div class="level-item">
      <%= link_to({ action: 'latest' }) do %>
        <span><%= t('.latest.title') %></span>
        <%= svg_tag 'angles-right' %>
      <% end %>
    </div>
  </div>
</div>
<div id="box" data-controller="single-input" class="fixed-grid has-2-cols">
  <%= render layout: 'show_layout', locals: local_assigns do %>
    <%= render partial: 'show_table', locals: local_assigns %>
  <% end %>
</div>

<%=
  tr_tag(
    model,
    {
      action: url_for(action: 'sync', id: model.id),
      method: :patch,
      icon: 'rotate',
      title: t('.sync.title')
    },
    {
      action: url_for(action: 'test', id: model.id),
      method: :patch,
      icon: 'vial',
      title: t('.test.title')
    },
    icons: []
  ) { yield }
%>

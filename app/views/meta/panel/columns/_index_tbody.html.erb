<td><%= model.column_name %></td>
<td>
  <p><%= model.column_type %></p>
  <p class="text-grey"><%= model.sql_type %></p>
</td>
<td><%= model.column_limit %></td>
<td>
<% if model.defined_db %>
  <%= svg_tag 'check', class: 'icon is-small text-success' %>
<% else %>
  <%= svg_tag 'xmark', class: 'icon is-small text-danger' %>
<% end %>
</td>
<td>
<% if model.defined_model %>
  <%= svg_tag 'check', class: 'icon is-small text-success' %>
<% else %>
  <%= svg_tag 'xmark', class: 'icon is-small text-danger' %>
<% end %>
</td>

<tr id="tr_<%= model.id %>" data-controller="show">
  <%= yield %>
  <td>
    <% cache_with_svg do %>
    <div data-show-target="item" class="visibility-hidden">
      <form class="button_to" method="post" action="">
        <button aria-label="详情" class="button is-small is-borderless" type="submit">
          <svg class="icon text-info"><use href="http://lvh.me:3000/assets/icons_light-01a8b140.svg#eye"></use></svg>
        </button>
      </form>
      <form class="button_to" method="post" action="">
        <button aria-label="编辑" class="button is-small is-borderless" type="submit">
          <svg class="icon text-link"><use href="http://lvh.me:3000/assets/icons_light-01a8b140.svg#pencil"></use></svg>
        </button>
      </form>
      <form data-turbo-confirm="确认删除" class="button_to" method="post" action="">
        <input type="hidden" name="_method" value="delete">
        <button aria-label="删除" class="button is-small is-borderless" type="submit">
          <svg class="icon text-danger"><use href="http://lvh.me:3000/assets/icons_light-01a8b140.svg#trash"></use></svg>
        </button>
      </form>
    </div>
    <% end %>
  </td>
</tr>

<%= render 'solid_queue_tabs' %>

<%= render layout: 'index_table' do %>
  <%= render partial: 'index_tbody', layout: 'index_tr', collection: @queues, as: :model %>
  <%= render layout: 'index_tr' do %>
    <td>
      <%= link_to({ controller: 'recurring_tasks' }) do %>
        <span><%= t('.recurring_tasks.index.title') %></span>
      <% end %>
    </td>
    <td colspan="3"></td>
  <% end %>
<% end %>

<%= render layout: 'process_table' do %>
  <%= render partial: 'process_tbody', layout: 'process_tr', collection: @processes, as: :model %>
<% end %>
<td class="family-monospace"><%= model.id %></td>
<td><%= model.name %></td>
<td><%= model.record_type %></td>
<td>
  <p class="family-monospace"><%= model.record_id %></p>
  <p class="family-monospace"><%= model.blob_id %></p>
</td>
<td><time data-controller="time"><%= model.created_at.to_fs(:iso8601) %></time></td>
<td>
  <div data-show-target="item" class="visibility-hidden">
    <%= button_to({ action: 'delete', id: model.id }, method: :delete, aria: { label: t('.destroy.title') }, class: 'button is-small is-borderless', form: { data: { turbo_confirm: t('.destroy.confirm') } }) do %>
      <%= svg_tag 'trash', class: 'icon text-danger' %>
    <% end %>
  </div>
</td>
